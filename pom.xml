        <module>elasticjob-api</module>
        <module>elasticjob-infra</module>
        
        <module>elasticjob-lite</module>
        <module>elasticjob-cloud</module>
        
        <module>elasticjob-distribution</module>
        <module>elasticjob-ecosystem</module>
    </modules>
    
